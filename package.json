{
  "name": "draggula",
  "version": "1.0.2",
  "license": "MIT",
  "main": "dist/index.js",
  "scripts": {
    "start": "tsc -w",
    "watch": "jest --watch",
    "test": "jest",
    "test:bin": "node dist/index.js",
    "release": "yarn build && git commit -am 'build'; yarn version && git push --tags && git push origin master",
    "build": "tsc"
  },
  "dependencies": {
    "lodash": "^4.17.5"
  },
  "peerDependencies": {
    "react": "^16.4.0",
    "react-native": "^0.55.4"
  },
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "@types/lodash": "^4.14.102",
    "@types/react": "^16.3.14",
    "@types/react-native": "^0.55.16",
    "jest": "^22.4.3",
    "ts-jest": "^22.4.5",
    "tslint-config-prettier": "^1.7.0",
    "typescript": "^2.8.3"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
